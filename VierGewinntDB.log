/*C3*/SET SCHEMA PUBLIC
DROP TABLE TURN
DROP TABLE Match
DROP TABLE GAME
CREATE TABLE GAME(G_ID INTEGER NOT NULL PRIMARY KEY,PLAYER1 VARCHAR(255), PLAYER2 VARCHAR(255),WINNER VARCHAR(255),POINTS INTEGER)
CREATE TABLE MATCH(M_ID INTEGER NOT NULL PRIMARY KEY,G_ID INTEGER NOT NULL,FOREIGN KEY(G_ID) REFERENCES PUBLIC.GAME(G_ID))
CREATE TABLE TURN(T_ID INTEGER NOT NULL PRIMARY KEY,M_ID INTEGER,PERSON VARCHAR(255),POS_Y INTEGER,FOREIGN KEY(M_ID) REFERENCES PUBLIC.MATCH(M_ID))
DISCONNECT
